FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1500;

deltaT          1;

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
(
	forces
	{
		type forces;
		functionObjectLibs 	("libforces.so");
		outputControl   	timeStep;
		outputInterval  	1;

		patches 			(FOIL);
		pName 				p;
		Uname 				U;
		log 				false;
		rhoName 			rhoInf;
		rhoInf 				1000;
		CofR 				(0 0 0);
	}
);

// ************************************************************************* //
