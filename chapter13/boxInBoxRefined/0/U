FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 1);

boundaryField
{
    REST
    {
        /*type            zeroGradient;*/
        type            fixedValue; 
        value           uniform (0 0 1);
    }
    ZMAX
    {
        /*type            zeroGradient;*/
        type            fixedValue; 
        value           uniform (0 0 1);
    }
    ZMIN 
    {
        /*type            zeroGradient;*/
        type            fixedValue; 
        value           uniform (0 0 1);
    }
    HULL
    {
        type            zeroGradient;
        /*type            movingWallVelocity;*/
        /*type            fixedValue; */
        /*value           uniform (0 0 0);*/
    }
}


