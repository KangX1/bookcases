FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;  

boundaryField
{
    REST
    {
        type            zeroGradient;
    }
    ZMAX
    {
        type            zeroGradient;
    }
    ZMIN
    {
        type            zeroGradient;
    }
    HULL
    {
        type            zeroGradient;
    }
}


