FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.3 0.3 0.3);

boundaryField
{
    sides
    {
        type            inletOutlet;
        value           uniform (0.3 0.3 0.3);
        inletValue      uniform (0.3 0.3 0.3);
    }
}

